<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itwillbs.member.ProMapper">

	<insert id="insertMember">
		insert into PROS(serviceId,addressId,genderId,name,pass,email,phone,carrer,employees,identity,business,certificate)
		values(#{serviceId},#{addressId},#{genderId},#{name},#{pass},#{email},#{phone},#{carrer},#{employees},#{identity},
			   #{business},#{certificate})
	</insert>
	

